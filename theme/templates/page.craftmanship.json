    "6044598f-93e7-416f-831a-071dc8a9bd21": {
      "type": "cols-image-text",
      "blocks": {
        "b1d94a3a-644a-41b8-aefa-04a9e74fe4cf": {
          "type": "Column",
          "settings": {
            "main_image": "shopify:\/\/shop_images\/Rectangle_2-1_ee8a024c-1769-4913-89b2-979ce17b1cd0.png",
            "text": "<p>The importance of preserving traditional techniques and skills cannot be overstated, as they form the foundation of the industry.<\/p>"
          }
        },
        "5a44de06-c4b5-405a-bc02-d3b871a28ab2": {
          "type": "Column",
          "settings": {
            "main_image": "shopify:\/\/shop_images\/Screenshot_2023-07-05_at_7.37.png",
            "text": "<p>The use of new tools and techniques helps to speed up production without sacrificing quality or artistry. <\/p>"
          }
        }
      },
      "block_order": [
        "b1d94a3a-644a-41b8-aefa-04a9e74fe4cf",
        "5a44de06-c4b5-405a-bc02-d3b871a28ab2"
